{
    "name": "swooinc/laravel-becoolcouriers",
    "description": "An opinionated Laravel wrapper for BeCool Couriers' API.",
    "keywords": [
        "swooinc",
        "laravel-becoolcouriers"
    ],
    "homepage": "https://github.com/swooinc/laravel-becoolcouriers",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Chin Leung",
            "email": "chin@swoo.ca",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "SwooInc\\BeCool\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SwooInc\\BeCool\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "SwooInc\\BeCool\\BeCoolServiceProvider"
            ]
        }
    }
}